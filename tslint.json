{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-plugin-prettier"
    ],
    "rules": {
        "object-literal-sort-keys": false,
        "no-console": [
            false
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "array-type": [
            true,
            "generic"
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "no-unused-expression": false,
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ]
    }
}
